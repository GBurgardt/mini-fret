 => {\n\n        const noteTest = getSimpleNote();\n\n        $('.guitar-string.string-1')[0]\n            .appendChild(\n                noteTest.elementNote\n            );\n\n        // Movimiento\n        noteTest.itIsMoving = true;\n    }\n\n});\n\n\n\n\n// Evento 'keydown'\ndocument.addEventListener(\n    'keydown',\n    e => {\n        var keycode = e.keyCode;\n\n        if (\n            keycode === 49 || \n            keycode === 50 || \n            keycode === 51 || \n            keycode === 52 || \n            keycode === 53\n        ) {\n            $(\n                `.fret.fret-${\n                    keycode - 50 + 2\n                }`\n            ).addClass('pressed');\n        }       \n    },\n    false\n);\n\n// Evento 'keyup'\ndocument.addEventListener(\n    'keyup',\n    e => {\n        var keycode = e.keyCode;\n\n        if (\n            keycode === 49 || \n            keycode === 50 || \n            keycode === 51 || \n            keycode === 52 || \n            keycode === 53\n        ) {\n            $(\n                `.fret.fret-${\n                    keycode - 50 + 2\n                }`\n            ).removeClass('pressed')\n        }       \n    },\n    false\n);"},"lineCount":null}},"hash":"fa87e343578e6c77373c97b31091e32b","cacheData":{"env":{}}}